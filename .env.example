# Copy to .env and fill with your actual values
# NEVER commit the actual .env file

# Database
DATABASE_URL=postgresql://user:password@localhost/dbname
REDIS_URL=redis://localhost:6379

# OAuth - Google
GOOGLE_CLIENT_ID=your-client-id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=your-secret-here
GOOGLE_REDIRECT_URI=http://localhost:8000/auth/google/callback

# OAuth - HubSpot
HUBSPOT_CLIENT_ID=your-hubspot-client-id
HUBSPOT_CLIENT_SECRET=your-hubspot-secret
HUBSPOT_REDIRECT_URI=http://localhost:8000/auth/hubspot/callback

# API Keys
ANTHROPIC_API_KEY=sk-ant-...
OPENAI_API_KEY=sk-...

# Security
JWT_SECRET_KEY=generate-a-long-random-string
JWT_ALGORITHM=HS256
JWT_EXPIRATION_MINUTES=30

# Encryption
ENCRYPTION_KEY=generate-another-long-random-string

# Environment
ENVIRONMENT=development
ALLOWED_ORIGINS=http://localhost:3000